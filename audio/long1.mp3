function startScroll(vlh, vspeed, vdelay, vindex) {
	var t;
	var p = false;
	var o = document.getElementById(vindex);
	o.innerHTML += o.innerHTML;
	o.onmouseover = function() {
		p = true;
	};
	o.onmouseout = function() {
		p = false;
	};
	o.scrollTop = 0;
	function vstart() {
		if (!p) o.scrollTop +=1;
		t = setInterval(vscrolling, vspeed);
	}
	function vscrolling() {
		if (o.scrollHeight != 0) {
		vlh = Math.floor(o.scrollHeight / 6);
		}
		if (o.scrollTop % vlh != 0) {
			o.scrollTop += 1;
			if (o.scrollTop >= vlh * 3)
			o.scrollTop = 0;
		}
		else {
		clearInterval(t);
		setTimeout(vstart, vdelay);
		}
	}
	setTimeout(vstart, vdelay);
}
