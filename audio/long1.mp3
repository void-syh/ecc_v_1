function contains(parentNode, childNode) {
	if (parentNode.contains) {
		return parentNode != childNode && parentNode.contains(childNode); }
		else {
			return !!(parentNode.compareDocumentPosition(childNode) & 16);
		}
}
function checkHover(e,target){
	var rel = getEvent(e).relatedTarget ,
	from = getEvent(e).fromElement ,
	to = getEvent(e).toElement;
	if (getEvent(e).type=="mouseover")  {
		return !contains(target,rel||from) && !( (rel||from)===target );
	} else {
		return !contains(target,rel||to) && !( (rel||to)===target );
	}
}
function getEvent(e){
	return e||window.event;
}
function MobileShow(){
	document.getElementById("vMobile_box").style.display="block";
}
function Mobile(){
	var times;
	var tt=document.getElementById("vMobile");
	var box=document.getElementById("vMobile_box");
	tt.onmouseover=function(e){
		if(checkHover(e,this)){
			times=setTimeout(function(){
				box.style.display="block";
				return false;
			},300)
		}
	}
	tt.onmouseout=function(e){
		if(checkHover(e,this)){
			clearTimeout(times);
			box.style.display="none";
			return false;
		}
	}
}
Mobile();
function checkFather(that,e){
	var parent =e.relatedTarget;
	try {
		while ( parent && parent !== that ) {
			parent = parent.parentNode;
		}
		return (parent !== that);
	} catch(e) { }
}

function setCookie(name,value,day){
    var Days = day || 1;
    var exp = new Date();
    exp.setTime(exp.getTime() + Days*24*60*60*1000);
    document.cookie = name + "="+ escape (value) + ";expires=" + exp.toGMTString();
}
 
function getCookie(name){
    var arr,reg=new RegExp("(^| )"+name+"=([^;]*)(;|$)");
    if(arr=document.cookie.match(reg)) return unescape(arr[2]);
    else return null;
}
 
function delCookie(name){
    var exp = new Date();
    exp.setTime(exp.getTime() - 1);
    var cval=getCookie(name);
    if(cval!=null) document.cookie= name + "="+cval+";expires="+exp.toGMTString();
}
